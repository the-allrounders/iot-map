{
  "name": "iot-map",
  "scripts": {
    "start":"yarn toolbox && next -p 3001",
    "dev": "yarn start",
    "dev:local": "yarn toolbox && SERVER_ADDR=http://localhost:3000 next -p 3001",
    "build": "yarn toolbox && next build && next export && touch out/.nojekyll",
    "serve": "next start",
    "toolbox": "react-toolbox-themr --config=./.toolbox"
  },
  "dependencies": {
    "babel-plugin-inline-import": "^2.0.6",
    "fetch-everywhere": "^1.0.5",
    "leaflet": "^1.2.0",
    "next": "^3.2.1",
    "prop-types": "latest",
    "react": "^15.6.1",
    "react-animated-number": "^0.4.3",
    "react-dom": "^15.6.1",
    "react-leaflet": "^1.6.5",
    "react-leaflet-marker-layer": "ianwensink/react-leaflet-marker-layer",
    "react-no-ssr": "^1.1.0",
    "react-toolbox": "^2.0.0-beta.12",
    "styled-components": "^3.1.0-2",
    "webpack": "latest"
  },
  "devDependencies": {
    "react-toolbox-themr": "^1.0.2"
  }
}
